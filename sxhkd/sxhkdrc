    autorandr -c

XF86MonBrightnessUp
    macbook-lighter-screen -i 100

XF86MonBrightnessDown
    macbook-lighter-screen -d 100

XF86KbdBrightnessUp
    macbook-lighter-kbd -i 50

XF86KbdBrightnessDown
    macbook-lighter-kbd -d 50



