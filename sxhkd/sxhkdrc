XF86AudioMute
    pactl set-sink-mute @DEFAULT_SINK@ toggle

XF86Audio{Raise,Lower}Volume
    pactl set-sink-volume @DEFAULT_SINK@ {+5%,-5%}

XF86LaunchB
    autorandr -c

XF86MonBrightnessUp
    macbook-lighter-screen -i 100

XF86MonBrightnessDown
    macbook-lighter-screen -d 100

XF86KbdBrightnessUp
    macbook-lighter-kbd -i 50

XF86KbdBrightnessDown
    macbook-lighter-kbd -d 50



